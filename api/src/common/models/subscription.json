{
  "name": "Subscription",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "promisify": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "longDescription": {
      "type": "string"
    },
    "thumbnail": {
      "type": "string"
    },
    "imageCover": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "password": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "buyer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "customerId"
    },
    "userGroups": {
      "type": "hasMany",
      "model": "UserGroup",
      "foreignKey": "subscriptionId",
      "through": "UserGroupSubscription"
    }
  },
  "acls": [
  ],
  "methods": {}
}
